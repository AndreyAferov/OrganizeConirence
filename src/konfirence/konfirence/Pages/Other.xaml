<Page x:Class="konfirence.Pages.Other"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:konfirence.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Other"
      Background="White">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="60"></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="BackButton" Content="Назад"
                Click="BackButton_Click"  HorizontalAlignment="Center"
                VerticalAlignment="Center" Grid.Row="1" 
                Width="180" Height="40" Margin="10"></Button>
    </Grid>
</Page>
